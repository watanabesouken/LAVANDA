}
.lavany {
  background: rgba(233,227,237,1);
  text-align: center;
  border-bottom: 1px solid #978ea1;
}
.lavany .main-img {
  background: #f4f4f4;
  margin: 0 calc(50% - 50vw);
  width: 100vw;
  padding: 100px 0 90px;
}
.lavany .main-img img {
	max-width: 1200px;
}
.main-contents .anniversary-video .anniv-img {
  padding: 70px 0;
}
.main-contents .anniversary-video .anniv-video {
  max-width: 655px;
}
.goods-one {
