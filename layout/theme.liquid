<head>{% render 'pagefly-head' %}

  <!-- Basic page needs ================================================== -->
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	
  <!-- Title and description ================================================== -->
  <title>
  {{ page_title }}{% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
  </title>

  {% if page_description %}
  <meta name="description" content="{{ page_description | escape | split: '[/countdown]' | last | strip_html }}">
  {% endif %}

  <!-- Product meta ================================================== -->
  {% include 'fb-open-graph-tags' %}
  {% include 'twitter-card' %}

  <!-- Helpers ================================================== -->
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0, minimal-ui">
  
  <!-- Favicon -->
  {% if settings.favicon != blank %}
  <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png">
  {% endif %}
  
  <!-- webcilip -->
  <link rel="apple-touch-icon" href="https://cdn.shopify.com/s/files/1/0322/8116/5869/files/apple-touch-icon.png?v=1615204912" sizes="180x180">
<link rel="icon" type="image/png" href="https://cdn.shopify.com/s/files/1/0322/8116/5869/files/android-touch-icon.png?v=1615204857" sizes="192x192">
 
   <!-- fonts -->
  {% include 'head_fonts' %}
  
  <!-- Styles -->
  {% include 'head_styles' %}
  
   <!-- Scripts -->
  {% include 'head_scripts' %}

  <!-- Header hook for plugins ================================ -->
  {{ content_for_header }}

  <!--[if lt IE 9]>
  {{ '//html5shiv.googlecode.com/svn/trunk/html5.js' | script_tag }}
  <![endif]-->

  {% comment %}
    If you store has customers disabled, you can remove the following JS file
  {% endcomment %}
  
  {% if template contains 'customers' %}
    {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {{ 'customer_area.js'  | shopify_asset_url | script_tag }}
  {% endif %}

  {% include 'linked-options' %}
  
{% comment %}
{% if template == "index" %}
 {% include "popup" %}
{% endif %}
{% endcomment %}

  
{% include 'pagefly-header' %}
  <link href="https://use.fontawesome.com/releases/v5.15.1/css/all.css" rel="stylesheet">

 </head> 
 
{%- assign layout_class = 'body-full' -%}   

<body id="{{ page_title | handle }}" class="{{ header_class }} {{ layout_class }} {% if template contains 'product' %}pdetail{% endif %} {% if template contains 'coordinate' or template contains 'yr_lavanda' %}special {% endif %} {% if customer %}customer-logged-in {% endif %}template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}">
    {% if template contains 'index' %}  
    <h1 style="display: none">{{ page_title | split: '|' | first }}</h1>
    {% endif %}
    {% include 'wrapper-header' %}   
	{% include 'scheme' %}
    <div class="wrapper-container wrapper-body">	
        <main class="main-content" role="main">      
            {{ content_for_layout }}
        </main>    
            
        {% section 'footer-5' %}

    </div>
    
    {% include 'footer_scripts' %}
    
    {% include 'sweettooth-widgets' %}

    {% include 'search-autocomplete' %}
    
    <div class="translate-header" data-translate-header>
        {% include 'customer_links' %}
        {% include 'wrapper-top-cart' %}
    </div>
            
    {% include 'newsletter' %}      
    
 
    
    <div class="wrapper-overlay"></div>

